{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "never"],
        "no-console": "error",
        "indent": ["error", 4],
        "import/extensions": [ {
            "js": "always"
          }]
    }
}
